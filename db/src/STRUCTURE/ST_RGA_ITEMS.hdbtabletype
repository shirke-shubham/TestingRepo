TYPE "ST_RGA_ITEMS" as TABLE (

"RGA_NO" BIGINT NOT NULL,
"RGA_ITEM_NO" BIGINT NOT NULL,
"ITEM_CODE" NVARCHAR(10),
"ITEM_DESCRIPTION" NVARCHAR(1000),
"UNIT_OF_MEASURE" NVARCHAR(3), 
"BATCH" NVARCHAR(10),
"EXPIRY_DATE" DATE,
"SALEABLE" NVARCHAR(1),     
"INVOICE_NO" NVARCHAR(15), 
"INVOICE_DATE" DATE,   
"INVOICE_QUANTITY" INTEGER,      
"PRICE" DOUBLE,
"EXTENDED" DOUBLE,
"RETURN_QUANTITY" INTEGER,
"ACCEPTED_QUANTITY" INTEGER,
"ACCEPTED_PRICE" DOUBLE,    
PRIMARY KEY("RGA_NO", "RGA_ITEM_NO")
)