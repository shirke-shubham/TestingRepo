TYPE "ST_GRN_HEADER" as TABLE (

  DISTRIBUTOR_ID NVARCHAR(100),
  DISTRIBUTOR_NAME NVARCHAR(100),
  DELIVERY_NO NVARCHAR(15),
  INVOICE_NO NVARCHAR(15) NOT NULL,
  INVOICE_DATE DATE,
  DELIVERY_DATE DATE,
  ACCEPTED_DATE DATE,
  INVOICE_AMOUNT DOUBLE,
  STATUS NVARCHAR(20),
  SAP_ORDER_NO NVARCHAR(10),
  PRIMARY KEY(INVOICE_NO)
)